  <DataSets>
    <DataSet Name="Sell">
      <Query>
        <DataSourceName>ContoroRetail</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Country">
            <Value>=Parameters!Country.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category">
            <Value>=Parameters!Category.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subcategory">
            <Value>=Parameters!Subcategory.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Quarter">
            <Value>=Parameters!Quarter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ProductName, SumRevenue, SumQuantity, Subcategory, CategoryKey, Category, FiscalQuarter, FiscalYear, RegionCountryName
FROM            V_ProductTopSellCategoryYear
WHERE        (RegionCountryName = @Country) and Category=@Category and Subcategory=@Subcategory and FiscalYear=@Year and FiscalQuarter=@Quarter
ORDER BY FiscalYear , FiscalQuarter ,Category
</CommandText>
      </Query>
